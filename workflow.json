{
  "initial_state": {
    "conversation": []
  },
  "nodes": [
    {
      "id": "user_input",
      "class": "UserInputNode",
      "config": {}
    },
    {
      "id": "web_search",
      "class": "WebSearchNode",
      "config": {
        "shared_key": "user_query",
        "api_key": "REPLACE_WITH_YOUR_TAVILY_API_KEY",
        "max_retries": 2,
        "wait": 1.0
      }
    },
    {
      "id": "llm_response",
      "class": "LLMResponseNode",
      "config": {
        "api_key": "REPLACE_WITH_YOUR_OPENAI_KEY",
        "max_retries": 2,
        "wait": 1.0
      }
    },
    {
      "id": "continue_conversation",
      "class": "ContinueNode",
      "config": {}
    }
  ],
  "edges": [
    {
      "from": "user_input",
      "to": "web_search",
      "condition": "True"
    },
    {
      "from": "web_search",
      "to": "llm_response",
      "condition": "True"
    },
    {
      "from": "llm_response",
      "to": "continue_conversation",
      "condition": "True"
    },
    {
      "from": "continue_conversation",
      "to": "user_input",
      "condition": "shared['continue_conversation'] == True"
    }
  ],
  "start": "user_input"
}